Algoritmo "soma2numeros"
// Descrição   : Soma de dois números inteiros

Var
// Seção de Declarações das variáveis 
   num1: inteiro
   num2: inteiro
   soma: inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
   num1 <- 5
   num2 <- 2
   soma <- num1 + num2
   escreva (soma)

Fimalgoritmo